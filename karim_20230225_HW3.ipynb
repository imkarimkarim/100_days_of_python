        "h = float(input(\"What is your height in cm? \"))\n",
        "w = float(input(\"What is your weight in kg? \"))\n",
        "\n",
        "# height in meter\n",
        "hm = h / 100\n",
        "\n",
        "bmi = w / (hm ** 2)\n",
        "print(\"Your BMI is:\", round(bmi, 2))\n"
      ]
    }
